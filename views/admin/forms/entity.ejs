<div class="form-group">
    <label for="inputEntityDefaultName">Name</label>
    <input type="text" class="form-control" id="inputEntityDefaultName" name="Name"
           aria-describedby="entityDefaultNameHelp" placeholder="Enter Entity Name"
           value="<%= (data) ? data.Name : ""  %>">
    <small id="entityDefaultNameHelp" class="form-text text-muted">The name of the entity</small>
</div>
<div class="form-group">
    <label for="inputEntityStateType">Entity Type</label><br>
    <select class="custom-select" aria-describedby="entityStateTypeHelp" name="type">
        <% for(var s in formData.entityTypes) { %>
        <option value="<%= formData.entityTypes[s].id %>" <%= (data) ? ((data.EntityType == formData.entityStates[s].id) ? "selected" : "") : ""  %>><%= formData.entityTypes[s].DefaultName %></option>
        <% } %>
    </select>
    <small id="entityStateTypeHelp" class="form-text text-muted">Entity State Type</small>
</div>
